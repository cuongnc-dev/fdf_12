<div>
  <h2 class="row_h2">
    <%= t "your_shop" %>
    <div class="btn_view_all_wrapper pull-right">
      <% if @domain.present? %>
        <%= link_to t("create_new_shop"),
          new_dashboard_shop_path(domain_id: @domain.id),
          class: "btn btn_view_all btn-warning" %>
      <% else %>
        <%= link_to t("create_new_shop"), new_dashboard_shop_path,
          class: "btn btn_view_all btn-warning" %>
      <% end %>
    </div>
  </h2>
</div>
<div class="col-md-12 padding-0 list-manage-shop">
  <%= render @shops %>
</div>
<%= paginate @shops %>
