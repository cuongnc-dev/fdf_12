<% if users.present? %>
  <% users.each do |user| %>
    <div class="manager-search col-md-12 col-sm-12 col-xs-12"
      data-email="<%= user.email %>">
      <div class="col-md-2 col-sm-2 col-xs-3">
        <%= image_tag user.avatar.url, class: "manager-img" %>
      </div>
      <div class="col-md-10 col-sm-10 col-xs-9">
        <div><%= user.name %></div>
        <%= user.email %>
      </div>
    </div>
  <% end %>
<% end %>
